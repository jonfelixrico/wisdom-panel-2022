{
  "name": "wisdom-panel-2022",
  "version": "3.1.1-SNAPSHOT",
  "description": "Panel for the Wisdom Discord Bot",
  "productName": "Wisdom",
  "author": "Felix <jonfelixrico@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "mocks": "nodemon -e ts,json --exec ts-node --skipProject ./mocks",
    "start:dev": "quasar dev",
    "start:dev:be": "quasar dev --proxyTargetUrl http://localhost:9085",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component"
  },
  "dependencies": {
    "@quasar/extras": "^1.15.8",
    "axios": "^0.21.4",
    "discord-api-types": "^0.37.23",
    "lodash": "^4.17.21",
    "pinia": "^2.0.28",
    "pino": "^8.8.0",
    "quasar": "^2.10.2",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@quasar/app-vite": "^1.1.3",
    "@quasar/quasar-app-extension-testing": "^2.0.6",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "5.0.0-beta.11",
    "@types/dotenv-defaults": "^2.0.1",
    "@types/express": "^4.17.15",
    "@types/express-session": "^1.17.5",
    "@types/lodash": "^4.14.191",
    "@types/node": "^12.20.55",
    "@types/yargs": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "autoprefixer": "^10.4.13",
    "dotenv-defaults": "^5.0.2",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.8.0",
    "express": "^4.18.2",
    "express-handlebars": "^6.0.6",
    "express-session": "^1.17.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "yargs": "^17.6.2",
    "cypress": "^12.2.0",
    "eslint-plugin-cypress": "^2.12.1"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
